{
    "name": "zikula/osnabrueck-theme",
    "version": "0.1.0",
    "description": "Theme for Oldtimer IG Osnabrueck based on bootstrap theme",
    "type": "zikula-theme",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Ralf",
            "homepage": "http://oldtimer-ig-osnabrueck.org/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\OsnabrueckTheme\\": "" }
    },
    "require": {
        "php": ">=5.4.1"
    },
    "extra": {
        "zikula" : {
            "core-compatibility": ">=1.4.1",
            "class": "Zikula\\OsnabrueckTheme\\ZikulaOsnabrueckTheme",
            "displayname": "Osnabrueck",
            "capabilities": {
                "user": true,
                "admin": true
            }
        }
    }
}
