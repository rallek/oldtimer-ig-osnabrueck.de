{
    "name": "rk/websitehelper-module",
    "version": "0.5.0",
    "description": "here you can find the helpers for this site e.g. carousel, linker or images",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Ralf Koester",
            "email": "ralf@familie-koester.de",
            "homepage": "http://oldtimer-ig-osnabrueck.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "RK\\WebsiteHelperModule\\": "" }
    },
    "require": {
        "php": ">=5.4.1"
    },
    "suggest": {
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.4.5",
            "class": "RK\\WebsiteHelperModule\\RKWebsiteHelperModule",
            "displayname": "Website helper",
            "url": "websitehelper",
            "capabilities": {
                "user": {"route": "rkwebsitehelpermodule_linker_index"},
                "admin": {"route": "rkwebsitehelpermodule_linker_adminindex"},
                "hook_subscriber": {"class": "RK\\WebsiteHelperModule\\Container\\HookContainer"}
            },
            "securityschema": {
                "RKWebsiteHelperModule::": "::",
                "RKWebsiteHelperModule:ItemListBlock:": "Block title::",
                "RKWebsiteHelperModule:Linker:": "Linker ID::",
                "RKWebsiteHelperModule:CarouselItem:": "CarouselItem ID::",
                "RKWebsiteHelperModule:Carousel:CarouselItem": "Carousel ID:CarouselItem ID:",
                "RKWebsiteHelperModule:Carousel:": "Carousel ID::",
                "RKWebsiteHelperModule:WebsiteImage:": "WebsiteImage ID::",
                "RKWebsiteHelperModule::Ajax": "::"
            }
        }
    }
}
